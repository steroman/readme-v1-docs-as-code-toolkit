{
  "name": "readme-sync-scripts",
  "version": "1.0.0",
  "description": "A collection of scripts to manage and sync a local 'docs-as-code' repository with the ReadMe.com API.",
  "main": "scripts/sync/main.js",
  "type": "commonjs",
  "scripts": {
    "sync": "node scripts/sync/main.js",
    "sync:dry-run": "node scripts/sync/main.js --dry-run",
    "hierarchy": "node scripts/hierarchy/main.mjs",
    "hierarchy:validate": "node scripts/hierarchy/main.mjs validate",
    "hierarchy:manifest": "node scripts/hierarchy/main.mjs manifest",
    "migrate:cleanup": "node scripts/migration/01-cleanup-guides.mjs",
    "migrate:normalize": "node scripts/migration/02-normalize-structure.mjs",
    "migrate:assets": "node scripts/migration/03-normalize-assets.mjs"
  },
  "keywords": [
    "readme",
    "docs-as-code",
    "sync",
    "api",
    "documentation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.598.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.23",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "p-map": "^4.0.0",
    "simple-git": "^3.25.0",
    "slugify": "^1.6.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}